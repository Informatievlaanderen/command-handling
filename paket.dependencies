// Only the target frameworks that are used in projects.
// If you change the target framework of the projects then you need to run paket install again.
framework: netcoreapp31

// When doing development as a non-employee, uncomment this to use the public NuGet feed
source https://api.nuget.org/v3/index.json

// When doing development as an employee, uncomment this to use the private NuGet proxy
// source https://informatievlaanderen.myget.org/F/nuget-mirror/api/v3/index.json username: "%MYGET_FEED_USER%" password: "%MYGET_FEED_PASS%" authtype: "basic"

// PRODUCTION STUFF
nuget SqlStreamStore 1.1.3
nuget SqlStreamStore.MsSql 1.1.3

nuget Microsoft.Extensions.Logging 3.1.0
nuget Microsoft.Extensions.Configuration 3.1.0
nuget Microsoft.Extensions.Options.ConfigurationExtensions 3.1.0

nuget Microsoft.EntityFrameworkCore 3.1.0
nuget Microsoft.EntityFrameworkCore.SqlServer 3.1.0
nuget Microsoft.EntityFrameworkCore.InMemory 3.1.0
nuget Microsoft.EntityFrameworkCore.Design 3.1.0

nuget Microsoft.Data.SqlClient 1.1.0

nuget Autofac.Extensions.DependencyInjection 5.0.1

nuget Newtonsoft.Json 12.0.3

nuget NodaTime 2.4.7

nuget CompareNETObjects 4.64.0

// VBR STUFF
nuget Be.Vlaanderen.Basisregisters.Build.Pipeline 3.2.0

nuget Be.Vlaanderen.Basisregisters.EventHandling 2.1.0

nuget Be.Vlaanderen.Basisregisters.Utilities.HashCodeCalculator 2.1.0
